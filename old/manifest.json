{
  "manifest_version": 3,
  "name": "Trading Helper",
  "description": "I am a trading helper that helps make da big money!",
  "version": "1.0",
  "icons": {
    "128": "images/enabled-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "128": "images/disabled-128.png"
    },
    "default_title": "Trading Helper",
    "default_popup": "disabled.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.example.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://*.example.com/*"]
}
